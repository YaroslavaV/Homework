package com.gmail.thepowerofyaroslava;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		int wall;
		System.out.println("Введите количество полос");
		wall = sc.nextInt();

		for (int i = 1; i <= wall; i++) {
			System.out.println("***+++***+++***+++***");
			sc.close();
		}

	}

}
