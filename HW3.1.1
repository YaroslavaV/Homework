package com.gmail.thepowerofyar;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);

		int a;
		System.out.println("Введите число");
		a = sc.nextInt();

		int n = 1;

		for (int i = 1; i <= a; i++) {
			n *= i;
			System.out.println(n);
			sc.close();
		}

	}

}
