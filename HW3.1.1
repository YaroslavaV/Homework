package com.gmail.thepowerofyaroslava;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		int i = 1;
		int wall;
		System.out.println("Введите количество полос");
		wall = sc.nextInt();

		while (i <= wall) {
			System.out.println("***+++***+++***+++***");
			i++;
		}

	}

}
