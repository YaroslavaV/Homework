package com.gmail.thepowerofyaroslava;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		int wall;
		System.out.println("Введите количество полос");
		wall = sc.nextInt();

		for (int i = 0; i < 5; i++) {
			for (int j = 0; j < wall; j++) {
				if (j % 2 == 0) {
					System.out.print("***");
				} else {
					System.out.print("+++");
				}
			}
			System.out.println();

		}
	}
}
