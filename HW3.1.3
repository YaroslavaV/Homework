package com.gmail.thepowerofyar;

public class Main {

	public static void main(String[] args) {

		int f = 5;
		int res;

		for (int i = 0; i <= 10; i++) {
			System.out.print(i + " * " + f + " = ");
			System.out.println(res = i * f);
		}

	}

}
